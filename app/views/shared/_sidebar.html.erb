<sidebar class="three">
  <% if current_events.size > 0 %>
    <div class="events">
      <h4><%= t('.coming_events') %></h4>
      <%= render partial: 'shared/event', collection: current_events %>
    </div>
    <div class="blog">
      <h4><%= t('.blog') %></h4>
    </div>
  <% else %>
    <h4><%= t('.not_content') %></h4>
  <% end %>  
</sidebar>