<sidebar class="three">
  <div class="events">
    <h4><%= t('.coming_events') %></h4>
    <span class="sidebar_events">
      <%= current_events.size > 0 ? (render partial: 'shared/event', collection: current_events) : t('.not_content') %>
    </span>
  </div>
  <div class="blog">
    <h4><%= t('.blog') %></h4>
    <span class="sidebar_blog">
      <%= t('.not_content') %>
    </span>    
  </div>
</sidebar>